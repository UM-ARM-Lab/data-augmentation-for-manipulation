    <arg name="gazebo" default="true"/>

    <arg name="pi_div2" value="$(eval pi/2)"/>
    <rosparam subst_value="true" param="left_arm_home">
        [-1.81695, -1.426, 0.749328, -1.55009, 2.84299, 0.258032, 2.742499]
    </rosparam>
    <rosparam subst_value="true" param="right_arm_home">
        [-1.5708, 1.27796, -0.853131, 1.59958, -0.331705, 0.243396, 2.912269]
    </rosparam>

    <node name="world_interaction" pkg="victor_3d_rope_shim" type="world_interaction">
        <param name="get_planning_scene_topic" value="gazebo/get_planning_scene" if="$(arg gazebo)"/>
    </node>
    <node name="fake_control_mode_interface" pkg="victor_3d_rope_shim" type="minimal_fake_control_mode_interface.py" if="$(arg gazebo)"/>

</launch>
