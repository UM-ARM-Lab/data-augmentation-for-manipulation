<%size = 2%>
<?xml version="1.0" ?>
<sdf version="1.5">
<model name="arena_<%=size%>m">
    <static>true</static>
    <link name="north_wall">
      <pose>0 <%=size/2%> 0.5 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <box>
            <size><%=size+0.1%> 0.1 1</size>
          </box>
        </geometry>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size><%=size+0.1%> 0.1 1</size>
          </box>
        </geometry>
      </collision>
    </link>
    <link name="south_wall">
      <pose>0 <%=-size/2%> 0.5 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <box>
            <size><%=size+0.1%> 0.1 1</size>
          </box>
        </geometry>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size><%=size+0.1%> 0.1 1</size>
          </box>
        </geometry>
      </collision>
    </link>
    <link name="east_wall">
      <pose><%=size/2%> 0 0.5 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.1 <%=size+0.1%> 1</size>
          </box>
        </geometry>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.1 <%=size+0.1%> 1</size>
          </box>
        </geometry>
      </collision>
    </link>
    <link name="west_wall">
      <pose><%=-size/2%> 0 0.5 0 0 0</pose>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.1 <%=size+0.1%> 1</size>
          </box>
        </geometry>
      </visual>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.1 <%=size+0.1%> 1</size>
          </box>
        </geometry>
      </collision>
    </link>
  </model>
</sdf>

