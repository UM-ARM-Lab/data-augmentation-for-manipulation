<sdf version='1.6'>
    <world name='default'>
        <scene>
            <grid>false</grid>
            <origin_visual>false</origin_visual>
        </scene>
        <gui>
            <camera name='user_camera'>
                <pose frame=''>2.89 -2.13 2.55 0 0.56 2.34</pose>
            </camera>
        </gui>

        <physics name="ode" type="ode">
            <ode>
                <constraints>
                    <contact_max_correcting_vel>0</contact_max_correcting_vel>
                </constraints>
            </ode>
            <max_step_size>0.002</max_step_size>
            <real_time_update_rate>0</real_time_update_rate>
        </physics>

        <plugin name="stepping_plugin" filename="libstepping_plugin.so"/>
        <plugin name="collision_map_plugin" filename="libcollision_map_plugin.so"/>
        <plugin name="gazebo_ros_tf_plugin" filename="libgazebo_ros_tf_plugin.so">
            <model_names>table val_and_rope</model_names>
        </plugin>

        <model name="val_and_rope">
            <include>
                <pose>0.4 0.6 0.5 0 0 3.141592</pose>
                <uri>model://kinematic_rope</uri>
            </include>

            <include>
                <uri>model://val</uri>
            </include>
            <plugin name="gazebo_ros_moveit_planning_scene" filename="libgazebo_ros_moveit_planning_scene.so">
                <topicName>gazebo/planning_scene</topicName>
                <robotName>hdt_michigan</robotName>
                <frameId>val_root</frameId>
                <updatePeriod>0</updatePeriod>
                <scalePrimitivesFactor>1.1</scalePrimitivesFactor>
            </plugin>
        </model>

        <include>
            <uri>model://sun</uri>
        </include>

        <include>
            <pose>0.0 0.0 -0.45 0 0 0</pose>
            <uri>model://my_ground_plane</uri>
        </include>

        <include>
            <pose>0.35 1.15 -0.45 0 0 0</pose>
            <uri>model://box2</uri>
        </include>

        <include>
            <pose>-0.40 0.5 -0.45 0 0 0</pose>
            <uri>model://hook1</uri>
        </include>

        <include>
            <pose>-0.55 0.77 -0.45 0 0 0</pose>
            <uri>model://hook2</uri>
        </include>

        <include>
            <pose>0.50 0.75 -0.45 0 0 0</pose>
            <uri>model://car_alternator</uri>
        </include>

        <include>
            <pose>0.0 0.75 -0.45 0 0 0</pose>
            <uri>model://car_engine2</uri>
        </include>

        <include>
            <pose>0.38 0.44 -0.45 0 0 0</pose>
            <uri>model://car_pulley</uri>
        </include>

        <include>
            <pose>0 1 0 -1.0 0 0</pose>
            <uri>model://car_hood</uri>
        </include>

    </world>
</sdf>
