<sdf version='1.6'>
    <world name='default'>
        <gui>
            <camera name='user_camera'>
                <pose frame=''>2.89 -2.13 2.55 0 0.56 2.34</pose>
            </camera>
        </gui>
        <include>
            <uri>model://sun</uri>
        </include>
        <include>
            <pose>0.0 0.0 -0.3 0 0 0</pose>
            <uri>model://my_ground_plane</uri>
        </include>
        <plugin name="stepping_plugin" filename="libstepping_plugin.so"/>
        <physics name="ode" type="ode">
            <ode>
                <constraints>
                    <contact_max_correcting_vel>0</contact_max_correcting_vel>
                </constraints>
            </ode>
            <max_step_size>0.002</max_step_size>
            <real_time_update_rate>0</real_time_update_rate>
        </physics>

        <model name="planning_scene">
            <plugin name="gazebo_ros_moveit_planning_scene" filename="libgazebo_ros_moveit_planning_scene.so">
                <topicName>gazebo/planning_scene</topicName>
                <robotName>hdt_michigan</robotName>
                <frameId>val_root</frameId>
                <updatePeriod>0.1</updatePeriod>
                <scalePrimitivesFactor>1.1</scalePrimitivesFactor>
            </plugin>
        </model>
        <plugin name="gazebo_ros_tf_plugin" filename="libgazebo_ros_tf_plugin.so"/>
        <plugin name="collision_map_plugin" filename="libcollision_map_plugin.so"/>

        <include>
            <uri>model://table</uri>
        </include>

        <include>
            <uri>model://cubes</uri>
        </include>
        <include>
            <uri>model://bottle</uri>
        </include>
        <include>
            <uri>model://mustard</uri>
        </include>
        <include>
            <uri>model://can</uri>
        </include>
        <include>
            <uri>model://cheezits</uri>
        </include>

        <model name="safety walls">
            <pose>0 4 0 0 0 -1.5707</pose>
            <static>true</static>
            <link name="1212_west_monitors">
                <pose>2.85 -1.1 1.5 0 0 0</pose>
                <collision name="collision">
                    <geometry>
                        <box>
                            <size>5 0.1 0.8</size>
                        </box>
                    </geometry>
                </collision>
                <visual name="visual">
                    <geometry>
                        <box>
                            <size>5 0.1 0.8</size>
                        </box>
                    </geometry>
                </visual>
            </link>
            <link name="1212_east_monitors">
                <pose>2.85 1.1 1.5 0 0 0</pose>
                <collision name="collision">
                    <geometry>
                        <box>
                            <size>5 0.1 0.8</size>
                        </box>
                    </geometry>
                </collision>
                <visual name="visual">
                    <geometry>
                        <box>
                            <size>5 0.1 0.8</size>
                        </box>
                    </geometry>
                </visual>
            </link>
            <link name="behind_victor">
                <pose>0.0 0 1.5 0 0 0</pose>
                <collision name="collision">
                    <geometry>
                        <box>
                            <size>0.1 2.4 0.8</size>
                        </box>
                    </geometry>
                </collision>
                <visual name="visual">
                    <geometry>
                        <box>
                            <size>0.1 2.4 0.8</size>
                        </box>
                    </geometry>
                </visual>
            </link>
        </model>
    </world>
</sdf>
