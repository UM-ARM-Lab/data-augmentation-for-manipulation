<?xml version="1.0" ?>
<sdf version="1.4">
    <world name="default">
        <scene>
            <grid>false</grid>
            <origin_visual>false</origin_visual>
            <sky>
                <clouds>
                    <speed>12</speed>
                </clouds>
            </sky>
        </scene>

        <physics name="ode" type="ode">
            <max_step_size>0.01</max_step_size>
            <real_time_factor>1</real_time_factor>
            <real_time_update_rate>100</real_time_update_rate>
            <ode>
                <solver>
                    <island_threads>2</island_threads>
                    <type>quick</type>
                    <iters>100</iters>
                    <sor>1.0</sor> <!-- Important, see issue #2209 -->
                    <use_dynamic_moi_rescaling>false</use_dynamic_moi_rescaling>
                </solver>
                <constraints>
                    <erp>0.8</erp>
                </constraints>
            </ode>
        </physics>

        <plugin name="stepping_plugin" filename="libstepping_plugin.so"/>
        <plugin name="gazebo_ros_tf_plugin" filename="libgazebo_ros_tf_plugin.so"/>
        <plugin name="ros_link_attacher_plugin" filename="libgazebo_ros_link_attacher.so"/>
        <plugin name="collision_map_plugin" filename="libcollision_map_plugin.so"/>

        <include>
            <pose>0 0 0 0 0 0</pose>
            <uri>model://my_ground_plane</uri>
        </include>

        <include>
            <uri>model://sun</uri>
        </include>

        <include>
            <pose>0.8 0 0.625 0 0 0</pose>
            <uri>model://victor_table</uri>
        </include>

        <include>
            <pose>0.8 -0.1 0.7 0 0</pose>
            <uri>model://002_master_chef_can</uri>
        </include>
        <include>
            <pose>0.9 0.1 0.7 0 0</pose>
            <uri>model://beer</uri>
        </include>

<!--        <include>-->
<!--            <pose>0.8 0.0 0.7 0 0</pose>-->
<!--            <uri>model://035_power_drill</uri>-->
<!--        </include>-->

<!--        <include>-->
<!--            <pose>0.8 0.1 0.6 0 0</pose>-->
<!--            <uri>model://003_cracker_box</uri>-->
<!--        </include>-->

        <include>
            <pose>1.8 0.0 1.2 0 0.15 3.14159</pose>
            <uri>model://kinect</uri>
        </include>
    </world>
</sdf>