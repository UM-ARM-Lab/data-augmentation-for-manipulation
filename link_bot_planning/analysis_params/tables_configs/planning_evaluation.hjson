[
  {
    type: MyTable
    name: success
    header: [
      Method Name
      Num Success
      Num Trials
      Success %
      Mean Task Error
      Median Task Error
      Num (re)plans
      Planning_Time
      Num Recovery Actions
      Mean NME
      Median NME
    ]
    reductions: [
      ["success", ["sum"]]
      ["num_trials", ["sum"]]
      ["success", ["mean"]]
      ["task_error", ["mean"]]
      ["task_error", ["median"]]
      ["num_planning_attempts", ["mean"]]
      ["planning_time", ["mean"]]
      ["num_recovery_actions", ["mean"]]
      ["normalized_model_error", ["mean"]]
      ["normalized_model_error", ["median"]]
    ]
  }
  {
    type: MyTable
    name: task_error
    header: [
      Method Name
      Task Error
    ]
    reductions: [
      ["task_error", ["mean"]]
    ]
  }
  {
    type: PValuesTable
    name: task_error
    reductions: [
      ["task_error", [null]]
    ]
  }
  {
    type: MyTable
    name: success_ratio
    header: [
      Method Name
      '# Success'
      '# Trials'
    ]
    reductions: [
      ["success", ["sum"]]
      ["num_trials", ["sum"]]
    ]
  }
]
