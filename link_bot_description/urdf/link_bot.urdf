<?xml version="1.0"?>
<robot name="myfirst">
  <link name="link_0">
    <visual>
      <origin rpy="0 -1.5707 0" xyz="0.5 0 0"/>
      <geometry>
        <cylinder length="1" radius="0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 -1.5707 0" xyz="0.5 0 0"/>
      <geometry>
        <box size="0.2 0.2 1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <!-- 1/3*m*l^2 = 1/3*10*1^2 = 10/3 -->
      <inertia ixx="3.333" ixy="0" ixz="0" iyy="0" iyz="0" izz="3.333"/>
      <origin rpy="0 0 0" xyz="0.5 0 0"/>
    </inertial>
  </link>
  <link name="link_1">
    <visual>
      <origin rpy="0 -1.5707 0" xyz="0.5 0 0"/>
      <geometry>
        <cylinder length="1" radius="0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 -1.5707 0" xyz="0.5 0 0"/>
      <geometry>
        <box size="0.2 0.2 1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <!-- 1/3*m*l^2 = 1/3*10*1^2 = 10/3 -->
      <inertia ixx="3.333" ixy="0" ixz="0" iyy="0" iyz="0" izz="3.333"/>
      <origin rpy="0 0 0" xyz="0.5 0 0"/>
    </inertial>
  </link>
  <joint name="joint" type="revolute">
    <parent link="link_0"/>
    <child link="link_1"/>
    <origin xyz="1 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2" upper="2" effort="10" velocity="1"/>
    <dynamics damping="5.0" friction="0.0"/>
  </joint>
  <gazebo reference="link_0">
    <mu1>0.4</mu1>
    <mu2>0.4</mu2>
    <material>Gazebo/Gray</material>
  </gazebo>
  <gazebo reference="link_1">
    <mu1>0.4</mu1>
    <mu2>0.4</mu2>
    <material>Gazebo/Green</material>
  </gazebo>
</robot>
