


<?xml version="1.0"?>
<robot name="link_bot">
    
    <link name="link_0">
        <visual>
            <origin rpy="0 -1.5707 0" xyz="0.235 0 0"/>
            <geometry>
                <cylinder length="0.49" radius="0.02"/>
            </geometry>
        </visual>
        <collision name="link_0_collision">
            <origin rpy="0 -1.5707 0" xyz="0.235 0 0"/>
            <geometry>
                <box size="0.04 0.04 0.47"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="5.0"/>
            <inertia ixx="3.333" ixy="0" ixz="0" iyy="0" iyz="0" izz="3.333"/>
            <origin rpy="0 0 0" xyz="0.48 0 0"/>
        </inertial>
    </link>
    
    <link name="link_1">
        <visual>
            <origin rpy="0 -1.5707 0" xyz="0.235 0 0"/>
            <geometry>
                <cylinder length="0.49" radius="0.02"/>
            </geometry>
        </visual>
        <collision name="link_1_collision">
            <origin rpy="0 -1.5707 0" xyz="0.235 0 0"/>
            <geometry>
                <box size="0.04 0.04 0.47"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="5.0"/>
            <inertia ixx="3.333" ixy="0" ixz="0" iyy="0" iyz="0" izz="3.333"/>
            <origin rpy="0 0 0" xyz="0.48 0 0"/>
        </inertial>
    </link>
    
    <link name="link_2">
        <visual>
            <origin rpy="0 -1.5707 0" xyz="0.235 0 0"/>
            <geometry>
                <cylinder length="0.49" radius="0.02"/>
            </geometry>
        </visual>
        <collision name="link_2_collision">
            <origin rpy="0 -1.5707 0" xyz="0.235 0 0"/>
            <geometry>
                <box size="0.04 0.04 0.47"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="5.0"/>
            <inertia ixx="3.333" ixy="0" ixz="0" iyy="0" iyz="0" izz="3.333"/>
            <origin rpy="0 0 0" xyz="0.48 0 0"/>
        </inertial>
    </link>
    

    <link name="head">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.02"/>
            </geometry>
        </visual>
        <collision name="head">
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.02 0.02 0.02"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="0.0004" ixy="0" ixz="0" iyy="0.0004" iyz="0" izz="0.004"/>
        </inertial>
    </link>

    
    <joint name="joint_0" type="revolute">
        <parent link="link_0"/>
        <child link="link_1"/>
        <origin xyz="0.48 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3" upper="3" effort="10" velocity="5"/>
        <dynamics damping="2.0" friction="0.01"/>
    </joint>
    
    <joint name="joint_1" type="revolute">
        <parent link="link_1"/>
        <child link="link_2"/>
        <origin xyz="0.48 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3" upper="3" effort="10" velocity="5"/>
        <dynamics damping="2.0" friction="0.01"/>
    </joint>
    

    <joint name="head_joint" type="revolute">
        <parent link="link_2"/>
        <child link="head"/>
        <origin xyz="0.48 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3" upper="3" effort="10" velocity="5"/>
        <dynamics damping="2.0" friction="0.01"/>
    </joint>

    
    <gazebo reference="link_0">
        <self_collide>true</self_collide>
        <sensor name='link_0_contact' type='contact'>
            <contact>
                <collision>link_0_collision</collision>
            </contact>
        </sensor>
    </gazebo>
    
    <gazebo reference="link_1">
        <self_collide>true</self_collide>
        <sensor name='link_1_contact' type='contact'>
            <contact>
                <collision>link_1_collision</collision>
            </contact>
        </sensor>
    </gazebo>
    
    <gazebo reference="link_2">
        <self_collide>true</self_collide>
        <sensor name='link_2_contact' type='contact'>
            <contact>
                <collision>link_2_collision</collision>
            </contact>
        </sensor>
    </gazebo>
    

    <gazebo reference="head">
        <self_collide>true</self_collide>
        <material>Gazebo/Blue</material>
        <sensor name='head_contact' type='contact'>
            <contact>
                <collision>head_collision</collision>
            </contact>
        </sensor>
        <visual>
            <plugin name="link_bot_visual" filename="liblinkbot_visual_plugin.so">
            </plugin>
        </visual>
    </gazebo>

    <gazebo reference="head_joint">
    </gazebo>

    <gazebo>
        <plugin name="multi_link_bot_model_plugin" filename="libmulti_link_bot_model_plugin.so">
            <kP_pos>2.5</kP_pos>
            <kI_pos>0.00</kI_pos>
            <kD_pos>0.0</kD_pos>
            <kP_vel>5000.0</kP_vel>
            <kI_vel>0.0</kI_vel>
            <kD_vel>0.0</kD_vel>
            <gripper1_link>head</gripper1_link>
        </plugin>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <legacyModeNS>false</legacyModeNS>
            <robotNamespace>link_bot</robotNamespace>
        </plugin>
    </gazebo>
