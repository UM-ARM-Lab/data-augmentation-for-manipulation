<?xml version='1.0'?>
<sdf version='1.6'>
    <model name='car'>
        <link name="center">
            <pose>0 0 0.01 0 0 0</pose>
            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <mass>1e-20</mass>
                <inertia>
                    <ixx>1e-20</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <izz>1e-20</izz>
                    <iyz>0</iyz>
                    <iyy>1e-20</iyy>
                </inertia>
            </inertial>
            <visual name="visual">
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.001 0.001 0.001</size>
                    </box>
                </geometry>
                <material>
                    <script>
                        <name>Gazebo/Blue</name>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                    </script>
                </material>
            </visual>
        </link>
        <link name='base'>
            <pose>0 0 0.005 0 0 0</pose>
            <inertial>
                <pose frame=''>0.0095 0.0 0.019 0 0 0</pose>
                <mass>0.5</mass>
                <inertia>
                    <ixx>2.8333e-4</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <izz>6.6570e-4</izz>
                    <iyz>0</iyz>
                    <iyy>4.6770e-4</iyy>
                </inertia>
            </inertial>
            <visual name='whole_robot_visual'>
                <pose>0 0.032 0.015 1.5707 0 0</pose>
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
                <material>
                    <script>
                        <name>Gazebo/Yellow</name>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                    </script>
                </material>
            </visual>
        </link>
        <link name='right_wheel'>
            <pose>0.0 -0.0313 0.021 0 1.57079 1.57079</pose>
            <inertial>
                <mass>0.01</mass>
                <inertia>
                    <ixx>6.96908333e-07</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>6.96908333e-07</iyy>
                    <iyz>0</iyz>
                    <izz>1.125e-06</izz>
                </inertia>
                <pose>0 0 0 0 0 0</pose>
            </inertial>
            <visual name='right_wheel_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.0145</radius>
                        <length>0.0127</length>
                    </cylinder>
                </geometry>
                <material>
                    <script>
                        <name>Gazebo/DarkGray</name>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                    </script>
                </material>
            </visual>
            <collision name='right_wheel_collision'>
                <pose>0 0 0 0 0 0</pose>
                <max_contacts>2</max_contacts>
                <geometry>
                    <cylinder>
                        <radius>0.0145</radius>
                        <length>0.0127</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.32</mu>
                            <mu2>0.48</mu2>
                            <fdir1>0 0 1</fdir1>
                            <slip1>0</slip1>
                            <slip2>0</slip2> <!-- increases "drifting" -->
                        </ode>
                    </friction>
                    <contact>
                        <ode>
                            <soft_cfm>1.7e-10</soft_cfm>
                            <soft_erp>0.17</soft_erp>
                            <kp>1e9</kp>
                            <kd>5e10</kd>
                            <max_vel>0.01</max_vel>
                            <min_depth>0</min_depth>
                        </ode>
                    </contact>
                </surface>
            </collision>
        </link>
        <link name='left_wheel'>
            <pose>0.0 0.0313 0.021 0 1.57079 1.57079</pose>
            <inertial>
                <mass>0.01</mass>
                <inertia>
                    <ixx>6.96908333e-07</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>6.96908333e-07</iyy>
                    <iyz>0</iyz>
                    <izz>1.125e-06</izz>
                </inertia>
            </inertial>
            <visual name='left_wheel_visual'>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.0145</radius>
                        <length>0.0127</length>
                    </cylinder>
                </geometry>
                <material>
                    <script>
                        <name>Gazebo/DarkGray</name>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                    </script>
                </material>
            </visual>
            <collision name='left_wheel_collision'>
                <max_contacts>2</max_contacts>
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.0145</radius>
                        <length>0.0127</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.32</mu>
                            <mu2>0.48</mu2>
                            <fdir1>0 0 1</fdir1>
                            <slip1>0</slip1>
                            <slip2>0</slip2>
                        </ode>
                    </friction>
                    <contact>
                        <ode>
                            <soft_cfm>1.7e-10</soft_cfm>
                            <soft_erp>0.17</soft_erp>
                            <kp>1e9</kp>
                            <kd>5e10</kd>
                            <max_vel>0.01</max_vel>
                            <min_depth>0</min_depth>
                        </ode>
                    </contact>
                </surface>
            </collision>
        </link>
        <joint name='left_wheel_joint' type='revolute'>
            <parent>base</parent>
            <child>left_wheel</child>
            <pose>0 0 0 0 0 0</pose>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <effort>0.016</effort> <!-- 16 kg-cm -->
                    <velocity>41.88</velocity> <!-- ~400 rpm -->
                </limit>
                <dynamics>
                    <damping>0.00058</damping>
                    <!--<friction>0.00040</friction>-->
                </dynamics>
            </axis>
            <physics>
                <ode>
                    <implicit_spring_damper>true</implicit_spring_damper>
                    <limit>
                        <cfm>0.0</cfm>
                        <erp>0.0</erp>
                    </limit>
                    <suspension>
                        <cfm>0.0</cfm>
                        <erp>0.0</erp>
                    </suspension>
                </ode>
            </physics>
        </joint>
        <joint name='right_wheel_joint' type='revolute'>
            <parent>base</parent>
            <child>right_wheel</child>
            <pose>0 0 0 0 0 0</pose>
            <axis>
                <xyz>0 0 1</xyz>
                <use_parent_model_frame>0</use_parent_model_frame>
                <limit>
                    <effort>0.016</effort> <!-- 16 kg-cm -->
                    <velocity>41.88</velocity> <!-- ~400 rpm -->
                </limit>
                <dynamics>
                    <damping>0.00058</damping>
                    <!--<friction>0.00040</friction>-->
                </dynamics>
            </axis>
            <physics>
                <ode>
                    <implicit_spring_damper>true</implicit_spring_damper>
                    <limit>
                        <cfm>0</cfm>
                        <erp>0.0</erp>
                    </limit>
                    <suspension>
                        <cfm>0</cfm>
                        <erp>0.0</erp>
                    </suspension>
                </ode>
            </physics>
        </joint>
        <joint name="center_joint" type="fixed">
            <parent>base</parent>
            <child>center</child>
            <pose>0 0 0 0 0 0</pose>
        </joint>
        <plugin name="car_plugin" filename="libcar_plugin.so">
            <left_wheel_joint>left_wheel_joint</left_wheel_joint>
            <right_wheel_joint>right_wheel_joint</right_wheel_joint>
            <link>center</link>
        </plugin>
    </model>
</sdf>
